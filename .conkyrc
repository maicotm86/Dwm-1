conky.config = {
	background = false,
	out_to_x = false,
	out_to_console = true,
	update_interval = 0.5,
	use_spacer = "none",
	cpu_avg_samples = 2,
	short_units = true,
}

conky.text = [[\
 ${downspeedf wlp2s0} \
 ${upspeedf wlp2s0}    \
${cpu}%    \
${memperc}% \
${swapperc}%    \
${battery_percent}%    \
${addr wlp2s0}    \
${exec /home/void/.vol.sh}%    \
${time %H:%M:%S %a %d/%m/%y} \
]]

--:${exec pacman -Qq | wc -l}   \
--:${fs_free /home}   \